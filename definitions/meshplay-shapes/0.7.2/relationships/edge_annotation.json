{
    "schemaVersion": "relationships.meshplay.io/v1alpha2",
    "version": "v1.0.0",
    "kind": "Edge",
    "metadata": {
        "description": "A relationship that defines annotation edges between components",
        "styles": {
            "lineStyle": "solid",
            "lineColor": "#00B39F",
            "target-arrow-color": "#00B39F",
            "targetArrowShape": "triangle"
        },
        "capabilities": {
            "designer": {
                "edit": {
                    "style": true,
                    "config": false,
                    "label": true
                }
            }
        }
    },
    "model": {
        "schemaVersion": "core.meshplay.io/v1beta1",
        "version": "v1.0.0",
        "name": "meshplay-shapes",
        "model": {
            "version": "0.7.2"
        },
        "displayName": "Meshery Shapes",
        "category": {
            "name": "Orchestration \u0026 Management",
            "metadata": null
        },
        "metadata": {}
    },
    "subType": "Annotation",
    "selectors": [
        {
            "allow": {
                "from": [
                    {
                        "kind": "*",
                        "model": "meshplay-core"
                    },
                    {
                        "kind": "*",
                        "model": "meshplay-shapes"
                    },
                    {
                        "kind": "*",
                        "model": "aws"
                    },
                    {
                        "kind": "*",
                        "model": "gcp"
                    },
                    {
                        "kind": "*",
                        "model": "meshplay-flowchart"
                    }
                ],
                "to": [
                    {
                        "kind": "*",
                        "model": "meshplay-shapes"
                    },
                    {
                        "kind": "*",
                        "model": "meshplay-core"
                    },
                    {
                        "kind": "*",
                        "model": "aws"
                    },
                    {
                        "kind": "*",
                        "model": "gcp"
                    },
                    {
                        "kind": "*",
                        "model": "meshplay-flowchart"
                    }
                ]
            },
            "deny": {
                "from": []
            }
        }
    ]
}